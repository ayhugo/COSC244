public static void main(String[] args) {

       int argsNum;

       if (args.length > 0) {
       try {
           argsNum = Integer.parseInt(args[1]);
           } catch (NumberFormatException e) {
             System.err.println("Argument " + args[1] + "must be an integer.");
             System.exit(1);
             }
}

        for (int i = 0; i < argsNum; i++){
        System.out.println("args[0]");
}
}
